{
  "name": "aws-lambda-cdk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "cdk bootstrap --app \"../mvnw -e -q compile exec:java -Dexec.mainClass=com.serverless.lambda.cdk.BootstrapApp\"",
    "app:deploy": "cdk deploy --app \"../mvnw -e -q compile exec:java -Dexec.mainClass=com.serverless.lambda.cdk.AwsCdkApp\" --require-approval never",
    "app:destroy": "cdk destroy --app \"../mvnw -e -q compile exec:java -Dexec.mainClass=com.serverless.lambda.cdk.AwsCdkApp\" --force --require-approval never",
  },
  "devDependencies": {
    "aws-cdk": "2.164.1"
  },
  "engines": {
    "node": ">=16"
  }
}